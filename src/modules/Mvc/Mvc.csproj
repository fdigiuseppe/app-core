<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <PackageVersion>1.0.0</PackageVersion>
    <PackageId>app.core.Extensions.Mvc</PackageId>
    <AssemblyName>app.core.Extensions.Mvc</AssemblyName>
    <Authors>Massimo Di Paolo</Authors>
    <Description>Mvc support for Web Pages w/Razor</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>First release</PackageReleaseNotes>
    <Copyright>Copyright 2017 (c) Massimo Di Paolo. All rights reserved.</Copyright>
    <PackageTags>Mvc, web pages, Razor</PackageTags>
    <RootNamespace>core.Extensions.Mvc</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <NoWarn>1701;1702;1705;1591</NoWarn>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ExtensionBase\ExtensionBase.csproj" />
  </ItemGroup>

</Project>