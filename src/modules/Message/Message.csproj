<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp1.1</TargetFramework>
    <PackageVersion>1.0.0</PackageVersion>
    <PackageId>app.core.Extensions.Message</PackageId>
    <AssemblyName>app.core.Extensions.Message</AssemblyName>    
    <Authors>Massimo Di Paolo</Authors>
    <Description>Send/Receive message via email</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>First release</PackageReleaseNotes>
    <Copyright>Copyright 2017 (c) Massimo Di Paolo. All rights reserved.</Copyright>
    <PackageTags>extension message, email, smtp</PackageTags>
    <RootNamespace>core.Extensions.Message</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <NoWarn>1701;1702;1705;1591</NoWarn>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="app.core.Extensions.Base" Version="1.0.0" />    
  </ItemGroup>

</Project>